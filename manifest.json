{
    "action": {
        "default_icon": {
            "16": "icons/16.png",
            "19": "icons/19.png",
            "38": "icons/38.png",
            "48": "icons/48.png",
            "128": "icons/128.png"
        },
        "default_title": "Enable 汉宝宝"
    },
    "author": "hkproj@gmail.com",
    "background": {
        "service_worker": "dist/background.js"
    },
    "content_scripts": [
        {
            "all_frames": true,
            "js": [
                "dist/content.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "description": "汉宝宝: Your chinese language learning toolbox",
    "icons": {
        "16": "icons/16.png",
        "19": "icons/19.png",
        "38": "icons/38.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "manifest_version": 3,
    "name": "汉宝宝: Your chinese language learning toolbox",
    "permissions": [
        "storage",
        "tabs",
        "contextMenus",
        "unlimitedStorage"
    ],
    "version": "1.0.0",
    "web_accessible_resources": [
        {
            "matches": [
                "<all_urls>"
            ],
            "resources": [
                "public/*"
            ]
        }
    ]
}